<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" stylesheets="@../css/teacher_adding.css" xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx" fx:controller="kz.fizmat.teachers.adding.TeacherAddingController">
   <content>
      <StackPane fx:id="stackPane" styleClass="root">
         <children>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="1000.0" prefWidth="500.0">
               <children>
                  <TextField fx:id="surnameFX" alignment="CENTER" layoutX="88.0" layoutY="69.0" promptText="Фамилия" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="40.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="nameFX" alignment="CENTER" layoutX="88.0" layoutY="127.0" promptText="Имя" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="patronymicFX" alignment="CENTER" layoutX="88.0" layoutY="183.0" promptText="Отчество" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <DatePicker fx:id="birthDateFX" layoutX="88.0" layoutY="244.0" promptText="Дата рождения">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </DatePicker>
                  <HBox alignment="TOP_CENTER">
                     <children>
                        <RadioButton fx:id="genderMaleFX" mnemonicParsing="false" text="мужской">
                           <HBox.margin>
                              <Insets left="9.0" right="9.0" />
                           </HBox.margin>
                        </RadioButton>
                        <RadioButton fx:id="genderFemaleFX" mnemonicParsing="false" text="женский">
                           <HBox.margin>
                              <Insets left="9.0" right="9.0" />
                           </HBox.margin>
                        </RadioButton>
                     </children>
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </HBox>
                  <TextField fx:id="nationalityFX" alignment="CENTER" layoutX="88.0" layoutY="350.0" promptText="Национальность" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="educationFX" alignment="CENTER" layoutX="88.0" layoutY="399.0" promptText="Образование" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="pedagogicalExperienceFX" alignment="CENTER" layoutX="88.0" layoutY="456.0" promptText="Педагогический стаж" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="qualificationFX" alignment="CENTER" layoutX="88.0" layoutY="507.0" promptText="Квалификация" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <DatePicker fx:id="qualificationDateFX" layoutX="55.0" layoutY="552.0" promptText="Дата получения педквалификации">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </DatePicker>
                  <TextField fx:id="generalExperienceFX" alignment="CENTER" layoutX="84.0" layoutY="642.0" promptText="Общий стаж" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <HBox>
                     <children>
                        <RadioButton fx:id="fullTimeFX" mnemonicParsing="false" text="штатный">
                           <HBox.margin>
                              <Insets left="9.0" right="9.0" />
                           </HBox.margin>
                        </RadioButton>
                        <RadioButton fx:id="partTimeFX" mnemonicParsing="false" text="совместитель">
                           <HBox.margin>
                              <Insets left="9.0" right="9.0" />
                           </HBox.margin>
                        </RadioButton>
                     </children>
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </HBox>
                  <TextField fx:id="numberOfChildrenFX" alignment="CENTER" layoutX="88.0" layoutY="808.0" promptText="Количество детей" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="awardsFX" alignment="CENTER" promptText="Награды" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="maritalStatusFX" alignment="CENTER" promptText="Семейное положение" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="categoryFX" alignment="CENTER" promptText="Категория" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <RadioButton fx:id="isGraduatedFX" mnemonicParsing="false" text="Окончен ВУЗ">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </RadioButton>
                  <TextField fx:id="teachesInGradesFX" alignment="CENTER" layoutX="84.0" layoutY="685.0" promptText="Преподает в классах" styleClass="textfield-focus">
                     <VBox.margin>
                        <Insets bottom="9.0" top="9.0" />
                     </VBox.margin>
                  </TextField>
                  <HBox>
                     <children>
                        <Button fx:id="cancelFX" mnemonicParsing="false" onAction="#onCancel" styleClass="cancel" text="Отмена">
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                        <Button fx:id="addFX" mnemonicParsing="false" onAction="#onAddTeacher" styleClass="add" text="Добавить">
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
            <ProgressIndicator fx:id="progress" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="83.0" prefWidth="200.0" StackPane.alignment="CENTER" />
         </children>
      </StackPane>
   </content>
</ScrollPane>
